{
  "redirects": [
    {
      "source": "/articles/:slug*",
      "destination": "/:slug*",
      "permanent": true
    },
    {
      "source": "/articles/:slug*/",
      "destination": "/:slug*",
      "permanent": true
    },
    {
      "source": "/([a-z0-9][a-z0-9\\-]*[a-z0-9])/",
      "destination": "/$1",
      "statusCode": 301,
      "has": [
        {
          "type": "header",
          "key": "host"
        }
      ],
      "missing": [
        {
          "type": "query",
          "key": "page"
        }
      ]
    },
    {
      "source": "/_astro/_page_.DH_Rpls4.css",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/privacy-policy",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/articles/",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
        }
      ]
    },
    {
      "source": "/categories/:category",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:standard, max-snippet:300"
        }
      ]
    },
    {
      "source": "/categories/:category/:page",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:none, max-snippet:150"
        }
      ]
    },
    {
      "source": "/:slug*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
        }
      ]
    }
  ]
}